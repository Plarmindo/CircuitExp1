{
  "name": "test",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron-main.cjs",
  "scripts": {
    "dev": "vite --port 5175 --strictPort",
    "start": "cross-env VITE_DEV_PORT=5175 electron electron-main.cjs",
    "dev:all": "cross-env VITE_DEV_PORT=5175 concurrently -k -n VITE,ELECTRON -c cyan,magenta \"vite --port 5175 --strictPort\" \"wait-on http://localhost:5175 && electron electron-main.cjs\"",
    "build:ui": "vite build",
    "build": "npm run build:ui",
    "test": "vitest run",
    "test:watch": "vitest",
  "test:e2e": "playwright test",
  "test:e2e:headed": "playwright test --headed",
    "perf:tree": "tsx scripts/stress-tree.ts"
  },
  "dependencies": {
    "d3": "^7.9.0",
    "pixi.js": "^8.12.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.1",
    "@playwright/test": "^1.54.1",
    "@types/d3": "^7.4.3",
    "@types/node": "^22.5.4",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.6.0",
    "concurrently": "^9.0.0",
    "cross-env": "^7.0.3",
    "electron": "^37.2.4",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "tsx": "^4.20.4",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite": "^7.0.4",
    "vitest": "^2.0.5",
    "wait-on": "^7.2.0"
  }
}
