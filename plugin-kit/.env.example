# Plugin Development Environment Configuration

# Anthropic Claude API Configuration
ANTHROPIC_API_KEY=your_anthropic_api_key_here
ANTHROPIC_MODEL=claude-3-sonnet-20240229

# OpenAI API Configuration (for comparison)
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview

# Google Gemini API Configuration
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=gemini-pro

# Server Configuration
PORT=3000
NODE_ENV=development
LOG_LEVEL=debug

# Plugin Configuration
PLUGIN_NAME=anthropic-claude-plugin
PLUGIN_VERSION=1.0.0
PLUGIN_DESCRIPTION=Anthropic Claude AI integration for Trae IDE

# Development Settings
ENABLE_DEBUG=true
ENABLE_METRICS=true
ENABLE_CORS=true

# Testing Configuration
TEST_API_KEY=test_api_key_for_testing
TEST_PORT=3001

# Logging Configuration
LOG_DIR=./logs
LOG_FILE=plugin.log
LOG_MAX_SIZE=10MB
LOG_MAX_FILES=5