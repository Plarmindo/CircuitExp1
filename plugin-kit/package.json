  },
  "homepage": "https://github.com/CircuitExp1/plugin-kit-external#readme",
  "dependencies": {
    "@types/node": "^20.0.0",
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "fs-extra": "^11.1.0",
    "axios": "^1.5.0",
    "express": "^4.18.0",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "rate-limiter-flexible": "^3.0.0",
    "winston": "^3.10.0",
    "dotenv": "^16.3.0",
